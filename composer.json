{
    "name": "wyrihaximus/ratchet",
    "type":"cakephp-plugin",
    "description": "Ratchet websockets integration plugin for CakePHP",
    "homepage": "http://wyrihaximus.net/projects/cakephp/ratchet.html",
    "license": "MIT",
    "keywords": [
        "cakephp",
        "ratchet",
        "websockets",
        "cakephp3"
    ],
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        }
    ],
    "require": {
        "php": "^5.4.0||^7.0",
        "cakephp/cakephp": "^3.6",
        "igorw/get-in": "^1.0",
        "lcobucci/jwt": "^3.2",
        "react/promise": "^2.0",
        "voryx/thruway": "^0.5.2",
        "wyrihaximus/to-child-process-or-not-to-child-process": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\Ratchet\\": "src/"
        },
        "files": ["src/functions_include.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\Ratchet\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "cakephp/debug_kit": "^3.0",
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^1.5.6",
        "vectorface/dunit": "^2.0",
        "phake/phake": "^2.1"
    },
    "config": {
        "sort-packages": true
    }
}
